{
  "$schema": "./node_modules/@angular/cli/lib/config/schema.json",
  "version": 1,
  "newProjectRoot": "projects",
  "projects": {
    "ngx-mercury-notifier": {
      "projectType": "library",
      "root": "projects/ngx-mercury-notifier",
      "sourceRoot": "projects/ngx-mercury-notifier/src",
      "prefix": "lib",
      "architect": {
        "build": {
          "builder": "@angular-devkit/build-angular:ng-packagr",
          "options": {
            "tsConfig": "projects/ngx-mercury-notifier/tsconfig.lib.json",
            "project": "projects/ngx-mercury-notifier/ng-package.json"
          },
          "configurations": {
            "production": {
              "tsConfig": "projects/ngx-mercury-notifier/tsconfig.lib.prod.json"
            }
          }
        },
        "test": {
          "builder": "@angular-devkit/build-angular:karma",
          "options": {
            "codeCoverage": true,
            "main": "projects/ngx-mercury-notifier/src/test.ts",
            "tsConfig": "projects/ngx-mercury-notifier/tsconfig.spec.json",
            "karmaConfig": "projects/ngx-mercury-notifier/karma.conf.js"
          }
        }
      }
    },
    "demo-app1": {
      "projectType": "application",
      "schematics": {
        "@schematics/angular:component": {
          "inlineTemplate": true,
          "inlineStyle": true,
          "style": "scss",
          "skipTests": true
        },
        "@schematics/angular:class": {
          "skipTests": true
        },
        "@schematics/angular:directive": {
          "skipTests": true
        },
        "@schematics/angular:guard": {
          "skipTests": true
        },
        "@schematics/angular:interceptor": {
          "skipTests": true
        },
        "@schematics/angular:module": {
          "skipTests": true
        },
        "@schematics/angular:pipe": {
          "skipTests": true
        },
        "@schematics/angular:service": {
          "skipTests": true
        }
      },
      "root": "projects/demo-app1",
      "sourceRoot": "projects/demo-app1/src",
      "prefix": "app",
      "architect": {
        "build": {
          "builder": "@angular-devkit/build-angular:browser",
          "options": {
            "outputPath": "dist/demo-app1",
            "index": "projects/demo-app1/src/index.html",
            "main": "projects/demo-app1/src/main.ts",
            "polyfills": "projects/demo-app1/src/polyfills.ts",
            "tsConfig": "projects/demo-app1/tsconfig.app.json",
            "assets": [
              "projects/demo-app1/src/favicon.ico",
              "projects/demo-app1/src/assets"
            ],
            "styles": [
              "projects/demo-app1/src/styles.scss"
            ],
            "scripts": [],
            "vendorChunk": true,
            "extractLicenses": false,
            "buildOptimizer": false,
            "sourceMap": true,
            "optimization": false,
            "namedChunks": true
          },
          "configurations": {
            "production": {
              "fileReplacements": [
                {
                  "replace": "projects/demo-app1/src/environments/environment.ts",
                  "with": "projects/demo-app1/src/environments/environment.prod.ts"
                }
              ],
              "optimization": true,
              "outputHashing": "all",
              "sourceMap": false,
              "namedChunks": false,
              "extractLicenses": true,
              "vendorChunk": false,
              "buildOptimizer": true,
              "budgets": [
                {
                  "type": "initial",
                  "maximumWarning": "2mb",
                  "maximumError": "5mb"
                },
                {
                  "type": "anyComponentStyle",
                  "maximumWarning": "6kb",
                  "maximumError": "10kb"
                }
              ]
            }
          }
        },
        "serve": {
          "builder": "@angular-devkit/build-angular:dev-server",
          "options": {
            "browserTarget": "demo-app1:build"
          },
          "configurations": {
            "production": {
              "browserTarget": "demo-app1:build:production"
            }
          }
        },
        "extract-i18n": {
          "builder": "@angular-devkit/build-angular:extract-i18n",
          "options": {
            "browserTarget": "demo-app1:build"
          }
        }
      }
    },
    "demo-app2": {
      "projectType": "application",
      "schematics": {
        "@schematics/angular:component": {
          "inlineTemplate": true,
          "inlineStyle": true,
          "style": "scss",
          "skipTests": true
        },
        "@schematics/angular:class": {
          "skipTests": true
        },
        "@schematics/angular:directive": {
          "skipTests": true
        },
        "@schematics/angular:guard": {
          "skipTests": true
        },
        "@schematics/angular:interceptor": {
          "skipTests": true
        },
        "@schematics/angular:module": {
          "skipTests": true
        },
        "@schematics/angular:pipe": {
          "skipTests": true
        },
        "@schematics/angular:service": {
          "skipTests": true
        }
      },
      "root": "projects/demo-app2",
      "sourceRoot": "projects/demo-app2/src",
      "prefix": "app",
      "architect": {
        "build": {
          "builder": "@angular-devkit/build-angular:browser",
          "options": {
            "outputPath": "dist/demo-app2",
            "index": "projects/demo-app2/src/index.html",
            "main": "projects/demo-app2/src/main.ts",
            "polyfills": "projects/demo-app2/src/polyfills.ts",
            "tsConfig": "projects/demo-app2/tsconfig.app.json",
            "assets": [
              "projects/demo-app2/src/favicon.ico",
              "projects/demo-app2/src/assets"
            ],
            "styles": [
              "projects/demo-app2/src/styles.scss"
            ],
            "scripts": [],
            "vendorChunk": true,
            "extractLicenses": false,
            "buildOptimizer": false,
            "sourceMap": true,
            "optimization": false,
            "namedChunks": true
          },
          "configurations": {
            "production": {
              "fileReplacements": [
                {
                  "replace": "projects/demo-app2/src/environments/environment.ts",
                  "with": "projects/demo-app2/src/environments/environment.prod.ts"
                }
              ],
              "optimization": true,
              "outputHashing": "all",
              "sourceMap": false,
              "namedChunks": false,
              "extractLicenses": true,
              "vendorChunk": false,
              "buildOptimizer": true,
              "budgets": [
                {
                  "type": "initial",
                  "maximumWarning": "2mb",
                  "maximumError": "5mb"
                },
                {
                  "type": "anyComponentStyle",
                  "maximumWarning": "6kb",
                  "maximumError": "10kb"
                }
              ]
            }
          }
        },
        "serve": {
          "builder": "@angular-devkit/build-angular:dev-server",
          "options": {
            "browserTarget": "demo-app2:build"
          },
          "configurations": {
            "production": {
              "browserTarget": "demo-app2:build:production"
            }
          }
        },
        "extract-i18n": {
          "builder": "@angular-devkit/build-angular:extract-i18n",
          "options": {
            "browserTarget": "demo-app2:build"
          }
        }
      }
    }},
  "defaultProject": "ngx-mercury-notifier"
}
